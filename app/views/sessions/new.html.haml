%section.sign_in.container
  .row
    .span10.offset1
      = form_tag 'sign_in' do
        %header
          %h1 Sign in
        %fieldset
          = label_tag :email
          = email_field_tag :email, params[:email]
          = label_tag :password
          = password_field_tag :password, params[:password]
        %fieldset.action
          = submit_tag "Sign in", class: 'btn'
